#cc flag
CC =	gcc
CFLAGS = -g  -pipe  -O -W -Wall -Wpointer-arith -g -Wno-unused-parameter # -Werror 
CPP =	gcc -E
LINK =	$(CC)


#all include header
CORE_DEPS = src/os/rtmp_config.h \
	src/rtmp/rtmp_core.h 

#all include dir
CORE_INCS = -I src/core \
